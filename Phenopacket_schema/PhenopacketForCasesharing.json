{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": "string"
        },
        "subject": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string"
                },
                "dateOfBirth": {
                    "type": "string",
                    "format": "date-time"
                },
                "sex": {
                    "type": "string",
                    "enum": [
                        "MALE",
                        "FEMALE",
                        "UNKNOWN_SEX",
                        "OTHER_SEX"
                    ]
                },
                "timeAtLastEncounter": {
                    "type": "object",
                    "properties": {
                        "age": {
                            "type": "object",
                            "properties": {
                                "iso8601duration": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "iso8601duration"
                            ],
                            "additionalProperties": false
                        },
                        "ontologyClass": {
                            "type": "object",
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "pattern": "^HP:.*"
                                },
                                "label": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "id",
                                "label"
                            ],
                            "additionalProperties": false
                        }
                    },
                    "anyOf": [
                        {
                            "required": [
                                "age"
                            ]
                        },
                        {
                            "required": [
                                "ontologyClass"
                            ]
                        }
                    ],
                    "additionalProperties": false
                },
                "vitalStatus": {
                    "type": "object",
                    "properties": {
                        "status": {
                            "type": "string",
                            "enum": [
                                "ALIVE",
                                "UNKNOWN_STATUS",
                                "DECEASED"
                            ]
                        },
                        "timeOfDeath": {
                            "type": "object",
                            "properties": {
                                "timestamp": {
                                    "type": "string",
                                    "format": "date-time"
                                }
                            },
                            "additionalProperties": false
                        }
                    },
                    "additionalProperties": false,
                    "allOf": [
                        {
                            "if": {
                                "not": {
                                    "properties": {
                                        "status": {
                                            "const": "DECEASED"
                                        }
                                    },
                                    "required": [
                                        "status"
                                    ]
                                }
                            },
                            "then": {
                                "properties": {
                                    "timeOfDeath": false
                                }
                            }
                        }
                    ]
                }
            },
            "required": [
                "id"
            ],
            "additionalProperties": false
        },
        "phenotypicFeatures": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "type": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string",
                                "pattern": "^HP:.*"
                            },
                            "label": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "id",
                            "label"
                        ],
                        "additionalProperties": false
                    },
                    "onset": {
                        "type": "object",
                        "properties": {
                            "ontologyClass": {
                                "type": "object",
                                "properties": {
                                    "id": {
                                        "type": "string",
                                        "pattern": "^HP:.*"
                                    },
                                    "label": {
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "id",
                                    "label"
                                ],
                                "additionalProperties": false
                            },
                            "age": {
                                "type": "object",
                                "properties": {
                                    "iso8601duration": {
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "iso8601duration"
                                ],
                                "additionalProperties": false
                            }
                        },
                        "anyOf": [
                            {
                                "required": [
                                    "age"
                                ]
                            },
                            {
                                "required": [
                                    "ontologyClass"
                                ]
                            }
                        ],
                        "additionalProperties": false
                    },
                    "excluded": {
                        "type": "boolean"
                    },
                    "modifiers": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "ontologyClass": {
                                    "type": "object",
                                    "properties": {
                                        "id": {
                                            "type": "string",
                                            "pattern": "^HP:.*"
                                        },
                                        "label": {
                                            "type": "string"
                                        }
                                    },
                                    "required": [
                                        "id",
                                        "label"
                                    ],
                                    "additionalProperties": false
                                }
                            }
                        }
                    },
                    "resolution": {
                        "type": "object",
                        "properties": {
                            "ontologyClass": {
                                "type": "object",
                                "properties": {
                                    "id": {
                                        "type": "string",
                                        "pattern": "^HP:.*"
                                    },
                                    "label": {
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "id",
                                    "label"
                                ],
                                "additionalProperties": false
                            },
                            "age": {
                                "type": "object",
                                "properties": {
                                    "iso8601duration": {
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "iso8601duration"
                                ],
                                "additionalProperties": false
                            },
                            "timestamp": {
                                "type": "string",
                                "format": "date-time"
                            }
                        },
                        "anyOf": [
                            {
                                "required": [
                                    "age"
                                ]
                            },
                            {
                                "required": [
                                    "ontologyClass"
                                ]
                            },
                            {
                                "required": [
                                    "timestamp"
                                ]
                            }
                        ],
                        "additionalProperties": false
                    },
                    "description": {
                        "type": "string"
                    }
                },
                "required": [
                    "type"
                ],
                "additionalProperties": false
            },
            "minItems": 1
        },
        "measurements": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "assay": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string"
                            },
                            "label": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "id",
                            "label"
                        ],
                        "additionalProperties": false
                    },
                    "value": {
                        "type": "object",
                        "properties": {
                            "quantity": {
                                "type": "object",
                                "properties": {
                                    "unit": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "string"
                                            },
                                            "label": {
                                                "type": "string"
                                            }
                                        },
                                        "required": [
                                            "id",
                                            "label"
                                        ],
                                        "additionalProperties": false
                                    },
                                    "value": {
                                        "type": "number"
                                    },
                                    "referenceRange": {
                                        "type": "object",
                                        "properties": {
                                            "unit": {
                                                "type": "object",
                                                "properties": {
                                                    "id": {
                                                        "type": "string"
                                                    },
                                                    "label": {
                                                        "type": "string"
                                                    }
                                                },
                                                "required": [
                                                    "id",
                                                    "label"
                                                ],
                                                "additionalProperties": false
                                            },
                                            "low": {
                                                "type": "number"
                                            },
                                            "high": {
                                                "type": "number"
                                            }
                                        },
                                        "required": [
                                            "unit",
                                            "low",
                                            "high"
                                        ],
                                        "additionalProperties": false
                                    }
                                },
                                "additionalProperties": false,
                                "required": [
                                    "unit",
                                    "value"
                                ]
                            }
                        },
                        "required": [
                            "quantity"
                        ],
                        "additionalProperties": false
                    },
                    "timeObserved": {
                        "type": "object",
                        "properties": {
                            "ontologyClass": {
                                "type": "object",
                                "properties": {
                                    "id": {
                                        "type": "string",
                                        "pattern": "^HP:.*"
                                    },
                                    "label": {
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "id",
                                    "label"
                                ],
                                "additionalProperties": false
                            },
                            "age": {
                                "type": "object",
                                "properties": {
                                    "iso8601duration": {
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "iso8601duration"
                                ],
                                "additionalProperties": false
                            }
                        },
                        "anyOf": [
                            {
                                "required": [
                                    "age"
                                ]
                            },
                            {
                                "required": [
                                    "ontologyClass"
                                ]
                            }
                        ],
                        "additionalProperties": false
                    }
                },
                "required": [
                    "assay",
                    "value"
                ],
                "additionalProperties": false
            }
        },
        "interpretations": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "progressStatus": {
                        "type": "string",
                        "enum": [
                            "UNKNOWN_PROGRESS",
                            "IN_PROGRESS",
                            "COMPLETED",
                            "SOLVED",
                            "UNSOLVED"
                        ]
                    },
                    "diagnosis": {
                        "type": "object",
                        "properties": {
                            "disease": {
                                "type": "object",
                                "properties": {
                                    "id": {
                                        "type": "string",
                                        "pattern": "^OMIM:.*"
                                    },
                                    "label": {
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "id",
                                    "label"
                                ],
                                "additionalProperties": false
                            },
                            "genomicInterpretations": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "subjectOrBiosampleId": {
                                            "type": "string"
                                        },
                                        "interpretationStatus": {
                                            "type": "string",
                                            "enum": [
                                                "UNKNOWN_STATUS",
                                                "REJECTED",
                                                "CANDIDATE",
                                                "CONTRIBUTORY",
                                                "CAUSATIVE"
                                            ]
                                        },
                                        "variantInterpretation": {
                                            "type": "object",
                                            "properties": {
                                                "acmgPathogenicityClassification": {
                                                    "type": "string",
                                                    "enum": [
                                                        "NOT_PROVIDED",
                                                        "BENIGN",
                                                        "LIKELY_BENIGN",
                                                        "UNCERTAIN_SIGNIFICANCE",
                                                        "LIKELY_PATHOGENIC",
                                                        "PATHOGENIC"
                                                    ]
                                                },
                                                "variationDescriptor": {
                                                    "type": "object",
                                                    "properties": {
                                                        "id": {
                                                            "type": "string"
                                                        },
                                                        "label": {
                                                            "type": "string"
                                                        },
                                                        "description": {
                                                            "type": "string"
                                                        },
                                                        "geneContext": {
                                                            "type": "object",
                                                            "properties": {
                                                                "valueId": {
                                                                    "type": "string",
                                                                    "pattern": "^HGNC:.*"
                                                                },
                                                                "symbol": {
                                                                    "type": "string"
                                                                }
                                                            },
                                                            "required": [
                                                                "valueId",
                                                                "symbol"
                                                            ],
                                                            "additionalProperties": false
                                                        },
                                                        "expressions": {
                                                            "type": "array",
                                                            "items": {
                                                                "type": "object",
                                                                "properties": {
                                                                    "syntax": {
                                                                        "type": "string",
                                                                        "enum": [
                                                                            "hgvs.g",
                                                                            "hgvs.c",
                                                                            "hgvs.p",
                                                                            "hgvs.m",
                                                                            "hgvs.n",
                                                                            "hgvs.r",
                                                                            "hgvs"
                                                                        ]
                                                                    },
                                                                    "value": {
                                                                        "type": "string"
                                                                    }
                                                                },
                                                                "required": [
                                                                    "syntax",
                                                                    "value"
                                                                ],
                                                                "additionalProperties": false
                                                            }
                                                        },
                                                        "vcfRecord": {
                                                            "type": "object",
                                                            "properties": {
                                                                "genomeAssembly": {
                                                                    "type": "string",
                                                                    "enum": [
                                                                        "hg38",
                                                                        "hg19",
                                                                        "hg18"
                                                                    ]
                                                                },
                                                                "chrom": {
                                                                    "type": "string",
                                                                    "pattern": "^chr.*"
                                                                },
                                                                "pos": {
                                                                    "type": "string"
                                                                },
                                                                "ref": {
                                                                    "type": "string",
                                                                    "pattern": "^[ATGC]+$"
                                                                },
                                                                "alt": {
                                                                    "type": "string",
                                                                    "pattern": "^[ATGC]+$"
                                                                }
                                                            },
                                                            "required": [
                                                                "genomeAssembly"
                                                            ],
                                                            "additionalProperties": false
                                                        },
                                                        "moleculeContext": {
                                                            "type": "string"
                                                        },
                                                        "allelicState": {
                                                            "type": "object",
                                                            "properties": {
                                                                "id": {
                                                                    "type": "string",
                                                                    "pattern": "^(HP|GENO):.*$"
                                                                },
                                                                "label": {
                                                                    "type": "string"
                                                                }
                                                            },
                                                            "required": [
                                                                "id",
                                                                "label"
                                                            ],
                                                            "additionalProperties": false
                                                        },
                                                        "structuralType": {
                                                            "type": "object",
                                                            "properties": {
                                                                "id": {
                                                                    "type": "string"
                                                                },
                                                                "label": {
                                                                    "type": "string"
                                                                }
                                                            },
                                                            "required": [
                                                                "id",
                                                                "label"
                                                            ],
                                                            "additionalProperties": false
                                                        }
                                                    },
                                                    "required": [
                                                        "allelicState"
                                                    ],
                                                    "additionalProperties": false
                                                }
                                            },
                                            "required": [
                                                "variationDescriptor"
                                            ],
                                            "additionalProperties": false
                                        }
                                    },
                                    "required": [
                                        "interpretationStatus",
                                        "variantInterpretation"
                                    ],
                                    "additionalProperties": false
                                }
                            }
                        },
                        "required": [
                            "genomicInterpretations"
                        ]
                    },
                    "summary": {
                        "type": "string"
                    }
                },
                "required": [
                    "progressStatus",
                    "diagnosis"
                ],
                "additionalProperties": false
            }
        },
        "diseases": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "properties": {
                    "term": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string",
                                "pattern": "OMIM.*"
                            },
                            "label": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "id",
                            "label"
                        ],
                        "additionalProperties": false
                    },
                    "onset": {
                        "type": "object",
                        "properties": {
                            "ontologyClass": {
                                "type": "object",
                                "properties": {
                                    "id": {
                                        "type": "string",
                                        "pattern": "^HP:.*"
                                    },
                                    "label": {
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "id",
                                    "label"
                                ],
                                "additionalProperties": false
                            },
                            "age": {
                                "type": "object",
                                "properties": {
                                    "iso8601duration": {
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "iso8601duration"
                                ],
                                "additionalProperties": false
                            }
                        },
                        "anyOf": [
                            {
                                "required": [
                                    "age"
                                ]
                            },
                            {
                                "required": [
                                    "ontologyClass"
                                ]
                            }
                        ],
                        "additionalProperties": false
                    }
                },
                "required": [
                    "term"
                ],
                "additionalProperties": false
            }
        },
        "metaData": {
            "type": "object",
            "properties": {
                "created": {
                    "type": "string",
                    "format": "date-time"
                },
                "createdBy": {
                    "type": "string"
                },
                "resources": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string"
                            },
                            "name": {
                                "type": "string"
                            },
                            "url": {
                                "type": "string",
                                "format": "uri"
                            },
                            "version": {
                                "type": "string"
                            },
                            "namespacePrefix": {
                                "type": "string"
                            },
                            "iriPrefix": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [
                            "id",
                            "name",
                            "url",
                            "version",
                            "namespacePrefix"
                        ],
                        "additionalProperties": false
                    }
                },
                "phenopacketSchemaVersion": {
                    "type": "string"
                },
                "externalReferences": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string",
                                "pattern": "^PMID:.*"
                            },
                            "reference": {
                                "type": "string",
                                "pattern": "^http.*"
                            },
                            "description": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "id",
                            "reference",
                            "description"
                        ],
                        "additionalProperties": false
                    }
                }
            },
            "required": [
                "created",
                "createdBy",
                "resources",
                "phenopacketSchemaVersion"
            ],
            "additionalProperties": false
        }
    },
    "required": [
        "id",
        "subject",
        "interpretations",
        "metaData"
    ],
    "additionalProperties": false
}